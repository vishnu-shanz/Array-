import java.util.*;
import java.lang.Math;
class Main{
    public static void main(String[]args){
        int a[]={1,2,3,4};
        int b[]={4,3,2,5};
        Arrays.sort(a);
        Arrays.sort(b);
        if(Arrays.equals(a,b)){
            System.out.println("True");
        }
        else{
            System.out.println("False");
        }
    }
     }
